<AnchorPane prefHeight="526.0" prefWidth="1007.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="testing.controller.Accounts">
   <children>
      <TableView fx:id="userTable" layoutX="3.0" prefHeight="443.0" prefWidth="1007.0">
