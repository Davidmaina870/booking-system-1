            <TableColumn id="emailTableColumn" fx:id="emailTableColumn" prefWidth="118.0" text="email" />
            <TableColumn id="genderTableColumn" fx:id="genderTableColumn" minWidth="2.0" prefWidth="67.0" text="gender" />
            <TableColumn id="ip_addressTableColumn" fx:id="ip_addressTableColumn" prefWidth="75.0" text="ip_address" />
            <TableColumn id="is_adminTableColumn" fx:id="is_adminTableColumn" prefWidth="73.0" text="is_admin" />
            <TableColumn id="cityTableColumn" fx:id="cityTableColumn" prefWidth="102.0" text="city" />
